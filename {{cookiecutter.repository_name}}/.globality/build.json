{
  "params": {
    "environments": [
      {
        "filters": {
          "branches": {
            "only": "master"
          }
        },
        "name": "dev"
      }
    ],
    "name": "{{ cookiecutter.repository_name }}",
    "import-name": "{{ cookiecutter.package_name }}",
    "service_command": "serve",
    "service_port": 8080,
    "update_commands": [
      "globality-build trigger-model-retraining --environment $ENVIRONMENT --image ${AWS_ECR_DOMAIN}/{{ cookiecutter.repository_name }}:${CIRCLE_SHA1} --model-name {{ cookiecutter.repository_name }} --configuration ./active.json"
    ]
  },
  "type": "python-service"
}
